{
  "name": "advent",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "pnpm run test:py && pnpm run test:js",
    "test:js": "jest src/test_solutions.test.ts",
    "test:py": "./.venv/bin/pytest src",
    "run:js": "ts-node src/run_js.ts",
    "run:py": "./.venv/bin/python src/run_py.py",
    "run:js:watch": "ts-node src/run_js_watch.ts",
    "run:py:watch": "./.venv/bin/python src/run_py_watch.py",
    "setup:py": "python3 -m venv .venv && ./.venv/bin/pip install -r requirements.txt",
    "postinstall": "pnpm run setup:py",
    "scaffold": "python src/scaffold.py",
    "lint": "pnpm run lint:ts && pnpm run lint:py",
    "lint:ts": "eslint src --ext .ts",
    "lint:py": "./.venv/bin/flake8 src && ./.venv/bin/black src --check && ./.venv/bin/mypy --package src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "chokidar": "^4.0.3",
    "number-to-words": "^1.2.4"
  }
}
