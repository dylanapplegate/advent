  "main": "index.js",
  "scripts": {
    "test": "pnpm run test:py && pnpm run test:js",
    "test:js": "jest test_solutions.test.ts",
    "test:py": "./.venv/bin/pytest",
    "run:js": "ts-node run_js.ts",
    "run:py": "./.venv/bin/python run_py.py",
    "run:js:watch": "ts-node run_js_watch.ts",
    "run:py:watch": "./.venv/bin/python run_py_watch.py",
    "setup:py": "python3 -m venv .venv && ./.venv/bin/pip install -r requirements.txt",
    "postinstall": "pnpm run setup:py",
    "scaffold": "python scaffold.py",
    "lint": "pnpm run lint:ts && pnpm run lint:py",
    "lint:ts": "eslint . --ext .ts",
    "lint:py": "./.venv/bin/flake8 . && ./.venv/bin/black . --check && ./.venv/bin/mypy ."
  },
